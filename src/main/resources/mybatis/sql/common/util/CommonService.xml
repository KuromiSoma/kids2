<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kdc.mybatis.mapper.common.util.CommonServiceMapper">
  <select id="searchUserByColor" resultType="com.kdc.common.entity.db.UserMasterEntity">
    <include refid="com.kdc.mybatis.mapper.common.entity.UserMasterMapper.selectAllColumnsFromUserMaster"></include>
        WHERE
            line_color = #{linecolor}
        AND del_flg = 0
  </select>

  <select id="getUserIdSeq" resultType="integer">
		SELECT
		    nextval('user_id_seq')
  </select>
	
  <select id="getPlaceIdSeq" resultType="integer">
		SELECT
		    nextval('place_id_seq')
  </select>

  <select id="getGroupIdSeq" resultType="integer">
		SELECT
		    nextval('group_id_seq')
  </select>

</mapper>
